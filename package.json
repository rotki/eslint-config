{
  "name": "@rotki/eslint-config",
  "version": "4.5.0",
  "packageManager": "pnpm@10.29.3",
  "type": "module",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/rotki/eslint-config/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rotki/eslint-config.git"
  },
  "author": "Rotki Solutions GmbH <info@rotki.com>",
  "files": [
    "dist"
  ],
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": "./dist/index.mjs"
  },
  "scripts": {
    "build": "pnpm run gen && tsdown --clean --dts",
    "stub": "tsdown",
    "dev": "tsdown --watch & npx @eslint/config-inspector --config eslint.config.ts",
    "watch": "tsdown --watch",
    "lint": "eslint .",
    "prepack": "pnpm run build",
    "release": "bumpp -r --no-push",
    "prepare": "husky",
    "gen": "tsx scripts/typegen.ts",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "@intlify/eslint-plugin-vue-i18n": "^4.0.0",
    "@prettier/plugin-xml": "^3.4.1",
    "@rotki/eslint-plugin": ">=1.1.0",
    "eslint": ">=9.20.0",
    "eslint-plugin-storybook": ">=0.11.0"
  },
  "peerDependenciesMeta": {
    "@intlify/eslint-plugin-vue-i18n": {
      "optional": true
    },
    "@prettier/plugin-xml": {
      "optional": true
    },
    "@rotki/eslint-plugin": {
      "optional": true
    },
    "eslint-plugin-storybook": {
      "optional": true
    }
  },
  "dependencies": {
    "@antfu/install-pkg": "1.1.0",
    "@clack/prompts": "1.0.1",
    "@eslint-community/eslint-plugin-eslint-comments": "4.6.0",
    "@eslint/markdown": "7.5.1",
    "@stylistic/eslint-plugin": "5.8.0",
    "@typescript-eslint/eslint-plugin": "8.55.0",
    "@typescript-eslint/parser": "8.55.0",
    "@vitest/eslint-plugin": "1.6.7",
    "eslint-config-flat-gitignore": "2.1.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-flat-config-utils": "3.0.1",
    "eslint-merge-processors": "2.0.0",
    "eslint-plugin-antfu": "3.2.2",
    "eslint-plugin-format": "1.4.0",
    "eslint-plugin-html": "8.1.4",
    "eslint-plugin-import-lite": "0.5.1",
    "eslint-plugin-jsonc": "2.21.1",
    "eslint-plugin-n": "17.23.2",
    "eslint-plugin-no-only-tests": "3.3.0",
    "eslint-plugin-perfectionist": "5.5.0",
    "eslint-plugin-pnpm": "1.5.0",
    "eslint-plugin-prettier": "5.5.5",
    "eslint-plugin-regexp": "3.0.0",
    "eslint-plugin-unicorn": "63.0.0",
    "eslint-plugin-unused-imports": "4.4.1",
    "eslint-plugin-vue": "10.8.0",
    "eslint-plugin-yml": "3.1.2",
    "eslint-processor-vue-blocks": "2.0.0",
    "globals": "17.3.0",
    "jsonc-eslint-parser": "2.4.2",
    "local-pkg": "1.1.2",
    "prettier": "3.8.1",
    "vue-eslint-parser": "10.4.0",
    "yaml-eslint-parser": "2.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "20.4.1",
    "@commitlint/config-conventional": "20.4.1",
    "@eslint/config-inspector": "1.4.2",
    "@rotki/eslint-config": "workspace:*",
    "@types/node": "24.10.13",
    "@types/prompts": "2.4.9",
    "bumpp": "10.4.1",
    "eslint": "9.39.2",
    "eslint-typegen": "2.3.0",
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "rimraf": "6.1.2",
    "tsdown": "0.20.3",
    "tsx": "4.21.0",
    "typescript": "5.9.3"
  },
  "engines": {
    "node": ">=24 <25",
    "pnpm": ">=10 <11"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
