{
  "name": "@rotki/eslint-config",
  "version": "3.0.0",
  "packageManager": "pnpm@9.7.0",
  "type": "module",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/rotki/eslint-config/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rotki/eslint-config.git"
  },
  "author": "Rotki Solutions GmbH <info@rotki.com>",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "pnpm run typegen && tsup src/index.ts --format esm,cjs --clean --dts",
    "stub": "tsup src/index.ts --format esm",
    "dev": "tsup src/index.ts --format esm,cjs --watch & npx @eslint/config-inspector --config eslint.config.mjs",
    "watch": "tsup --format esm,cjs --watch",
    "lint": "pnpm run stub && eslint .",
    "prepack": "pnpm run build",
    "release": "bumpp -r --no-push",
    "prepare": "husky",
    "typegen": "esno scripts/typegen.ts",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "@intlify/eslint-plugin-vue-i18n": "^3.0.0",
    "@prettier/plugin-xml": "^3.4.1",
    "@rotki/eslint-plugin": ">=0.4.0",
    "eslint": ">=9.0.0",
    "eslint-plugin-cypress": ">=3.0.0",
    "eslint-plugin-storybook": "^0.6.15 || ^0.8.0"
  },
  "peerDependenciesMeta": {
    "@intlify/eslint-plugin-vue-i18n": {
      "optional": true
    },
    "@prettier/plugin-xml": {
      "optional": true
    },
    "@rotki/eslint-plugin": {
      "optional": true
    },
    "eslint-plugin-cypress": {
      "optional": true
    },
    "eslint-plugin-storybook": {
      "optional": true
    }
  },
  "dependencies": {
    "@antfu/eslint-define-config": "1.23.0-2",
    "@antfu/install-pkg": "0.3.4",
    "@clack/prompts": "0.7.0",
    "@eslint-types/typescript-eslint": "7.5.0",
    "@eslint-types/unicorn": "52.0.0",
    "@stylistic/eslint-plugin": "2.6.2",
    "@typescript-eslint/eslint-plugin": "8.0.1",
    "@typescript-eslint/parser": "8.0.1",
    "@vitest/eslint-plugin": "1.0.1",
    "eslint-config-flat-gitignore": "0.1.8",
    "eslint-config-prettier": "9.1.0",
    "eslint-flat-config-utils": "0.3.0",
    "eslint-merge-processors": "0.1.0",
    "eslint-plugin-antfu": "2.3.4",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-format": "0.1.2",
    "eslint-plugin-html": "8.1.1",
    "eslint-plugin-import-x": "3.1.0",
    "eslint-plugin-jsonc": "2.16.0",
    "eslint-plugin-markdown": "5.1.0",
    "eslint-plugin-n": "17.10.2",
    "eslint-plugin-no-only-tests": "3.1.0",
    "eslint-plugin-perfectionist": "3.1.3",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-unicorn": "55.0.0",
    "eslint-plugin-unused-imports": "4.1.3",
    "eslint-plugin-vue": "9.27.0",
    "eslint-plugin-yml": "1.14.0",
    "eslint-processor-vue-blocks": "0.1.2",
    "globals": "15.9.0",
    "jsonc-eslint-parser": "2.4.0",
    "local-pkg": "0.5.0",
    "prettier": "3.3.3",
    "vue-eslint-parser": "9.4.3",
    "yaml-eslint-parser": "1.2.3"
  },
  "devDependencies": {
    "@commitlint/cli": "19.4.0",
    "@commitlint/config-conventional": "19.2.2",
    "@eslint/config-inspector": "0.5.2",
    "@rotki/eslint-config": "workspace:*",
    "@types/eslint": "9.6.0",
    "@types/node": "20.14.15",
    "@types/prompts": "2.4.9",
    "bumpp": "9.5.1",
    "eslint": "9.9.0",
    "eslint-plugin-regexp": "2.6.0",
    "eslint-typegen": "0.3.0",
    "esno": "4.7.0",
    "husky": "9.1.4",
    "lint-staged": "15.2.8",
    "rimraf": "6.0.1",
    "tsup": "8.2.4",
    "typescript": "5.5.4"
  },
  "engines": {
    "node": ">=20 <21",
    "pnpm": ">=9 <10"
  },
  "lint-staged": {
    "*.{js,ts,vue,json,md}": "eslint"
  }
}
