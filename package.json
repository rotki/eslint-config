{
  "name": "@rotki/eslint-config",
  "version": "4.3.0",
  "packageManager": "pnpm@10.13.1",
  "type": "module",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/rotki/eslint-config/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rotki/eslint-config.git"
  },
  "author": "Rotki Solutions GmbH <info@rotki.com>",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "build": "pnpm run gen && tsdown --clean --dts",
    "stub": "tsdown",
    "dev": "tsdown --watch & npx @eslint/config-inspector --config eslint.config.ts",
    "watch": "tsdown --watch",
    "lint": "eslint .",
    "prepack": "pnpm run build",
    "release": "bumpp -r --no-push",
    "prepare": "husky",
    "gen": "tsx scripts/typegen.ts",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "@intlify/eslint-plugin-vue-i18n": "^4.0.0",
    "@prettier/plugin-xml": "^3.4.1",
    "@rotki/eslint-plugin": ">=1.1.0",
    "eslint": ">=9.20.0",
    "eslint-plugin-cypress": ">=4.0.0",
    "eslint-plugin-storybook": ">=0.11.0"
  },
  "peerDependenciesMeta": {
    "@intlify/eslint-plugin-vue-i18n": {
      "optional": true
    },
    "@prettier/plugin-xml": {
      "optional": true
    },
    "@rotki/eslint-plugin": {
      "optional": true
    },
    "eslint-plugin-cypress": {
      "optional": true
    },
    "eslint-plugin-storybook": {
      "optional": true
    }
  },
  "dependencies": {
    "@antfu/install-pkg": "1.1.0",
    "@clack/prompts": "0.11.0",
    "@eslint-community/eslint-plugin-eslint-comments": "4.5.0",
    "@eslint/markdown": "7.0.0",
    "@stylistic/eslint-plugin": "5.2.1",
    "@typescript-eslint/eslint-plugin": "8.38.0",
    "@typescript-eslint/parser": "8.38.0",
    "@vitest/eslint-plugin": "1.3.4",
    "eslint-config-flat-gitignore": "2.1.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-flat-config-utils": "2.1.0",
    "eslint-merge-processors": "2.0.0",
    "eslint-plugin-antfu": "3.1.1",
    "eslint-plugin-format": "1.0.1",
    "eslint-plugin-html": "8.1.3",
    "eslint-plugin-import-lite": "0.3.0",
    "eslint-plugin-jsonc": "2.20.1",
    "eslint-plugin-n": "17.21.0",
    "eslint-plugin-no-only-tests": "3.3.0",
    "eslint-plugin-perfectionist": "4.15.0",
    "eslint-plugin-pnpm": "1.1.0",
    "eslint-plugin-prettier": "5.5.3",
    "eslint-plugin-unicorn": "60.0.0",
    "eslint-plugin-unused-imports": "4.1.4",
    "eslint-plugin-vue": "10.3.0",
    "eslint-plugin-yml": "1.18.0",
    "eslint-processor-vue-blocks": "2.0.0",
    "globals": "16.3.0",
    "jsonc-eslint-parser": "2.4.0",
    "local-pkg": "1.1.1",
    "prettier": "3.6.2",
    "vue-eslint-parser": "10.2.0",
    "yaml-eslint-parser": "1.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@eslint/config-inspector": "1.1.0",
    "@rotki/eslint-config": "workspace:*",
    "@types/node": "22.16.5",
    "@types/prompts": "2.4.9",
    "bumpp": "10.2.0",
    "eslint": "9.31.0",
    "eslint-plugin-regexp": "2.9.0",
    "eslint-typegen": "2.2.1",
    "husky": "9.1.7",
    "lint-staged": "16.1.2",
    "rimraf": "6.0.1",
    "tsdown": "0.13.0",
    "tsx": "4.20.3",
    "typescript": "5.8.3"
  },
  "engines": {
    "node": ">=22 <23",
    "pnpm": ">=10 <11"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
